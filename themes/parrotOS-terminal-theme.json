{
	"$schema": "vscode://schemas/color-theme",
	"name": "Parrot Terminal",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#00ff80",
		"editorCursor.foreground": "#00ff80",
		"editor.selectionBackground": "#003300",
		"activityBar.background": "#001100",
		"sideBar.background": "#001100",
		"statusBar.background": "#001100",
		"statusBar.foreground": "#00ff80",
		"terminal.background": "#000000",
		"terminal.foreground": "#00ff80",
		"terminalCursor.foreground": "#00ff80"
	},
	"tokenColors": [
		{
			"scope": ["comment"],
			"settings": { "foreground": "#00aa55", "fontStyle": "italic" }
		},
		{
			"scope": ["keyword", "storage"],
			"settings": { "foreground": "#00ff80" }
		},
		{
			"scope": ["string"],
			"settings": { "foreground": "#66ff99" }
		},
		{
			"scope": ["variable", "identifier"],
			"settings": { "foreground": "#aaffcc" }
		},
		{
			"scope": ["constant.numeric"],
			"settings": { "foreground": "#55ff55" }
		},
		{
			"scope": ["entity.name.function"],
			"settings": { "foreground": "#00ffaa" }
		},
		{
			"scope": ["entity.name.type", "support.class"],
			"settings": { "foreground": "#00e6b8" }
		}
	]
}
